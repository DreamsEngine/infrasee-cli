{
  "name": "infrasee-cli",
  "version": "1.5.5",
  "description": "CLI tool to find all domains and resources using a specific IP address across multiple cloud providers",
  "main": "dist/cli.js",
  "bin": {
    "infrasee": "./dist/cli.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "cli",
    "cloudflare",
    "digitalocean",
    "coolify",
    "gcp",
    "google-cloud",
    "infrastructure",
    "ip",
    "dns",
    "devops"
  ],
  "author": "Dreams Engine Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DreamsEngine/infrasee-cli.git"
  },
  "bugs": {
    "url": "https://github.com/DreamsEngine/infrasee-cli/issues"
  },
  "homepage": "https://github.com/DreamsEngine/infrasee-cli#readme",
  "type": "commonjs",
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.2",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^24.3.1",
    "typescript": "^5.9.2"
